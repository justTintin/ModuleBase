<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <attr name="circleMargin" format="integer"></attr>
        <attr name="circleMaxProcess" format="integer"></attr>
        <attr name="circleProgressColor" format="color" />
        <attr name="roundProgressColor" format="color" />
        <attr name="circleColor" format="color" />
        <attr name="circleBackground" format="reference" />
        <attr name="roundBackground" format="reference" />
        <attr name="circleWidth" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="CircleSeekBar">
        <attr name="wheel_max_process" format="integer" />
        <attr name="wheel_cur_process" format="integer" />
        <attr name="wheel_reached_color" format="color" />
        <attr name="wheel_reached_width" format="dimension" />
        <attr name="wheel_reached_has_corner_round" format="boolean" />
        <attr name="wheel_unreached_color" format="color" />
        <attr name="wheel_unreached_width" format="dimension" />
        <attr name="wheel_pointer_color" format="color" />
        <attr name="wheel_pointer_radius" format="dimension" />
        <attr name="wheel_has_pointer_shadow" format="boolean" />
        <attr name="wheel_has_wheel_shadow" format="boolean" />
        <attr name="wheel_pointer_shadow_radius" format="dimension" />
        <attr name="wheel_shadow_radius" format="dimension" />
        <attr name="wheel_has_cache" format="boolean" />
        <attr name="wheel_can_touch" format="boolean" />
        <attr name="wheel_radius_background" format="reference" />
    </declare-styleable>
    <declare-styleable name="SlidingMenu">
        <attr name="mode">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="viewAbove" format="reference" />
        <attr name="viewBehind" format="reference" />
        <attr name="behindOffset" format="dimension" />
        <attr name="behindWidth" format="dimension" />
        <attr name="behindScrollScale" format="float" />
        <attr name="touchModeAbove">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="touchModeBehind">
            <enum name="margin" value="0" />
            <enum name="fullscreen" value="1" />
        </attr>
        <attr name="shadowDrawable" format="reference" />
        <attr name="shadowWidth" format="dimension" />
        <attr name="fadeEnabled" format="boolean" />
        <attr name="fadeDegree" format="float" />
        <attr name="selectorEnabled" format="boolean" />
        <attr name="selectorDrawable" format="reference" />
    </declare-styleable>

    <attr name="emptyText" format="reference|string"/>
    <attr name="emptyImgId" format="reference"/>
    <attr name="emptyImageVisible" format="boolean"/>

    <attr name="errorText" format="reference|string"/>
    <attr name="errorImgId" format="reference"/>
    <attr name="errorImageVisible" format="boolean"/>

    <attr name="noNetWorkText" format="reference|string"/>
    <attr name="noNetWorkImgId" format="reference"/>
    <attr name="noNetWorkImageVisible" format="boolean"/>

    <attr name="allTipTextColor" format="reference|color"/>
    <attr name="allTipTextSize" format="integer"/>
    <attr name="allPageBackgroundColor" format="reference|color"/>

    <attr name="reloadBtnText" format="reference|string"/>
    <attr name="reloadBtnTextSize" format="integer"/>
    <attr name="reloadBtnTextColor" format="reference|color"/>
    <attr name="reloadBtnBackgroundResource" format="reference"/>
    <attr name="reloadBtnVisible" format="boolean"/>
    <attr name="reloadClickArea">
        <enum name="full" value="20"/>
        <enum name="button" value="21"/>
    </attr>

    <declare-styleable name="LoadDataLayout">
        <attr name="loadingText" format="reference|string"/>
        <attr name="loadingTextSize" format="integer"/>
        <attr name="loadingTextColor" format="reference|color"/>
        <attr name="loadingViewLayoutId" format="reference"/>

        <attr name="emptyText"/>
        <attr name="emptyImgId"/>
        <attr name="emptyImageVisible"/>

        <attr name="errorText"/>
        <attr name="errorImgId"/>
        <attr name="errorImageVisible"/>

        <attr name="noNetWorkText"/>
        <attr name="noNetWorkImgId"/>
        <attr name="noNetWorkImageVisible"/>

        <attr name="allTipTextColor"/>
        <attr name="allTipTextSize"/>
        <attr name="allPageBackgroundColor"/>

        <attr name="reloadBtnText"/>
        <attr name="reloadBtnTextSize"/>
        <attr name="reloadBtnTextColor"/>
        <attr name="reloadBtnBackgroundResource"/>
        <attr name="reloadBtnVisible"/>
        <attr name="reloadClickArea"/>

    </declare-styleable>

    <declare-styleable name="SwipeLoadDataLayout">
        <attr name="emptyText"/>
        <attr name="emptyImgId"/>
        <attr name="emptyImageVisible"/>

        <attr name="errorText"/>
        <attr name="errorImgId"/>
        <attr name="errorImageVisible"/>

        <attr name="noNetWorkText"/>
        <attr name="noNetWorkImgId"/>
        <attr name="noNetWorkImageVisible"/>

        <attr name="allTipTextColor"/>
        <attr name="allTipTextSize"/>
        <attr name="allPageBackgroundColor"/>

        <attr name="reloadBtnText"/>
        <attr name="reloadBtnTextSize"/>
        <attr name="reloadBtnTextColor"/>
        <attr name="reloadBtnBackgroundResource"/>
        <attr name="reloadBtnVisible"/>
        <attr name="reloadClickArea"/>
    </declare-styleable>
</resources>